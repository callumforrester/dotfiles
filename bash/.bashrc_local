#
# ~/.bashrc_local
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

# Env varialbes
export PATH="$PATH:$HOME/bin"
export TERMINAL="xfce4-terminal"

# Aliases
alias ls="ls --color=auto"
alias ll="ls -l"
alias lla="ll -a"
alias intellij="idea"
alias cclip="xclip -selection clipboard"

